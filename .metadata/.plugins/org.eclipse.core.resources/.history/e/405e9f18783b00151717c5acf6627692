// create the Data Store for each basic info

Ext.define('com.dudeClothing.AccountStore', {
	extend : 'Ext.data.Store',
	autoLoad : true,
	autoSync : true,
	model : 'Account',
	proxy : {
		type : 'rest',
		url : '/admin/account/accounts',
		reader : {
			type : 'json',
			rootProperty : 'data'
		},
		writer : {
			type : 'json'
		}
	}
});

