package com.dudeclothing.admin.editor;

import java.beans.PropertyEditorSupport;

import com.dudeclothing.admin.model.TestObject;

public class UIEditor extends PropertyEditorSupport {
	// This will be called when user HTTP Post to server a field bound to
	// DepartmentVO
	@Override
	public void setAsText(String id) {
		TestObject d;
		switch (Integer.parseInt(id)) {
		case 1:
			d = new TestObject(1, "Human Resource");
			break;
		case 2:
			d = new TestObject(2, "Finance");
			break;
		case 3:
			d = new TestObject(3, "Information Technology");
			break;
		default:
			d = null;
		}
		this.setValue(d);
	}
}